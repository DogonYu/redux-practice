{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended", "prettier/react", "airbnb", "plugin:prettier/recommended"
    // 꼭 배열의 맨 마지막이여야 모든 중복 규칙을 비활성화 시킬 수 있습니다.
  ],
  "rules":{
    "react/jsx-filename-extension":
    ["error",
     { "extensions": [".js", ".jsx"] }
    ]
  }
}